@model dynamic[]
@foreach (var agency in Model)
{
    var index = -1;
    <div class="agency mt-3" aria-title="@agency.Name">
        <h4><a href="@agency.SearchLink">@agency.Name</a><small><small> (@agency.Applied)</small> - @agency.Accepted <small>(@agency.AcceptingRate %)</small> / @agency.Analyzed <small>(@agency.AnalyzingRate %)</small> / @agency.JobCount</small></h4>
        @if (agency.Running is int running)
        {
            @foreach (var method in agency.Methods)
            {
                index += 1;
                var current = index == running ? "btn-primary" : "btn-outline-primary";
                <button id="RM-@(agency.Name)-@(index)" class="btn @current mt-1" onclick="change_running('@agency.Name', @index)">@method</button>
            }
        }
    </div>
}
