@model dynamic[]
@foreach (var agency in Model)
{
    var index = -1;
    <div class="agency" aria-title="@agency.Name">
        <h3>@agency.Name</h3>
        @foreach (var method in agency.Methods)
        {
            index += 1;
            var current = index == method.Running ? "current" : "";
            <button id="RM-@(agency.AgenyID)-@(index)" class="@current" onclick="change_running(@agency.AgenyID, @index)">@method</button>
        }
    </div>
}
